@model IEnumerable<CarRentalApp.Models.Identity.AppUser>

<h2>Customer List</h2>

<table class="table">
    <thead>
        <tr>
            <th>Customer ID</th>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.Id</td>
                <td>@customer.FirstName @customer.LastName</td>
                <td>@customer.Email</td>
            </tr>
        }
    </tbody>
</table>
